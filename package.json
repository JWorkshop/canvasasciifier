{
  "name": "@jworkshop/canvasasciifier",
  "version": "0.0.4",
  "description":
    "A <pre> text React component which renders ascii from a canvas.",
  "author": "jworkshop <john.joydesign@gmail.com>",
  "keywords": ["canvas", "ascii"],
  "repository": {
    "type": "git",
    "url": "git://github.com/JWorkshop/canvasasciifier.git"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.7.2",
    "eslint-plugin-react": "^7.7.0",
    "install": "^0.10.4",
    "node-sass": "^4.5.0",
    "npm": "^5.7.1",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "main": "./lib/index.js",
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "build":
      "npm run build-css && babel src -d lib --copy-files --ignore *.scss"
  },
  "babel": {
    "presets": ["env", "react"],
    "plugins": ["transform-object-rest-spread"]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "plugins": ["react"],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
      "semi": 2,
      "no-empty": 0
    }
  }
}
